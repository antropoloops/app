{
  "id": "test",
  "format": "audioset-2",
  "meta": {
    "title": "Test",
    "description": "Set de prueba de Antropoloops"
  },
  "resources": {
    "default": {
      "audio": "https://antropoloops.github.io/audiosets/test/audio/{{filename}}.mp3",
      "covers": "https://antropoloops.github.io/audiosets/test/covers/{{filename}}.jpg"
    },
    "development": {
      "audio": "/tmp/test/audio/{{filename}}.mp3",
      "covers": "/tmp/test/covers/{{filename}}.jpg"
    }
  },
  "audio": {
    "bpm": 110,
    "defaults": {
      "loop": true
    }
  },
  "visuals": {
    "geoMapUrl": "https://unpkg.com/world-atlas@1.1.4/world/110m.json"
  },
  "keyboard": {
    "defaults": {
      "type": "gate"
    },
    "keyMap": {
      "q": {
        "clipId": "voice1"
      },
      "w": {
        "clipId": "rhythm1"
      },
      "e": {
        "clipId": "accomp1"
      },
      "r": {
        "clipId": "melody1"
      },
      "a": {
        "clipId": "voice2"
      },
      "s": {
        "clipId": "rhythm2"
      },
      "d": {
        "clipId": "accomp2"
      },
      "f": {
        "clipId": "melody2"
      },
      "z": {
        "clipId": "voice3"
      }
    }
  }
}

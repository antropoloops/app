{
  "id": "lik03",
  "format": "audioset-2",
  "meta": {
    "title": "Lik03",
    "description": "Antropoloops Lik Session 03"
  },
  "resources": {
    "default": {
      "audio": "https://antropoloops.github.io/audiosets/lik03/audio/{{filename}}.mp3",
      "covers": "https://antropoloops.github.io/audiosets/lik03/covers/{{filename}}.jpg"
    },
    "google": {
      "audio": "https://storage.googleapis.com/atpls-sets/lik/{{filename}}.mp3",
      "covers": "https://storage.googleapis.com/atpls-sets/lik/lik03-covers/{{filename}}.jpg"
    },
    "development": {
      "audio": "/tmp/lik03/audio/{{filename}}.wav",
      "covers": "/tmp/lik03/covers/{{filename}}.jpg"
    }
  },
  "audio": {
    "bpm": 125,
    "signature": [4, 4],
    "trackMaxVoices": 1,
    "quantize": true,
    "defaults": {
      "loop": true
    }
  },
  "control": {
    "groups": {
      "name": "audio.track"
    }
  },
  "visuals": {
    "geoMapUrl": "/geodata/110m.json",
    "geoMapOriginalUrl": "https://unpkg.com/world-atlas@1.1.4/world/110m.json",
    "imprint": false,
    "focus": {
      "lambda": 65,
      "verticalShift": -3.1,
      "scaleFactor": 1.9
    }
  }
}

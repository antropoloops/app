{
  "id": "continentes",
  "format": "audioset-2",
  "meta": {
    "title": "Continentes",
    "description": "Continentes - Sessi√≥n Antropoloops 01"
  },
  "resources": {
    "default": {
      "audio": "https://antropoloops.github.io/audiosets/continentes/audio/{{filename}}.mp3",
      "covers": "https://antropoloops.github.io/audiosets/continentes/covers/{{filename}}.jpg"
    },
    "google": {
      "audio": "https://storage.googleapis.com/atpls-sets/continentes/{{filename}}.mp3",
      "covers": "https://storage.googleapis.com/atpls-sets/continentes/{{filename}}.jpg"
    },
    "development": {
      "audio": "/tmp/continentes/audio/{{filename}}.wav",
      "covers": "/tmp/continentes/covers/{{filename}}.jpg"
    },
    "github": {
      "audio": "https://antropoloops.github.io/audiosets/continentes/{{filename}}.wav"
    }
  },
  "audio": {
    "bpm": 120,
    "defaults": {
      "loop": true
    },
    "signature": [4, 4],
    "trackMaxVoices": 1,
    "quantize": true
  },
  "control": {
    "groups": {
      "name": "audio.track"
    }
  },
  "visuals": {
    "geoMapUrl": "https://unpkg.com/world-atlas@1.1.4/world/110m.json",
    "imprint": false,
    "focus": {
      "lambda": -10,
      "verticalShift": 15,
      "scaleFactor": 1
    }
  },
  "keyboard": {
    "defaults": {
      "type": "gate"
    },
    "keyMap": {
      "q": {
        "clipId": "ossie",
        "type": "toggle"
      },
      "a": {
        "clipId": "orillas"
      },
      "z": {
        "clipId": "mocita"
      },
      "w": {
        "clipId": "nubian",
        "type": "toggle"
      },
      "s": {
        "clipId": "tabla"
      },
      "x": {
        "clipId": "pae"
      },
      "e": {
        "clipId": "mississipi",
        "type": "toggle"
      },
      "d": {
        "clipId": "camino"
      },
      "c": {
        "clipId": "mariola"
      },
      "r": {
        "clipId": "madinda",
        "type": "toggle"
      },
      "f": {
        "clipId": "ballade"
      },
      "v": {
        "clipId": "funeraire"
      },
      "t": {
        "clipId": "ed-lewis",
        "type": "toggle"
      },
      "g": {
        "clipId": "rueda"
      },
      "b": {
        "clipId": "agnus-dei"
      },
      "y": {
        "clipId": "indvumo",
        "type": "toggle"
      },
      "h": {
        "clipId": "ogbeler"
      },
      "n": {
        "clipId": "are-are"
      }
    }
  }
}

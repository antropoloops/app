{
  "id": "continentes",
  "format": "audioset-2",
  "meta": {
    "title": "Continentes",
    "description": "Sessi√≥n Antropoloops 01"
  },
  "loader": {
    "sources": {
      "audio": [
        "https://storage.googleapis.com/atpls-sets/continentes/{{filename}}.wav",
        "https://antropoloops.github.io/audiosets/continentes/{{filename}}.mp3"
      ],
      "covers": [
        "https://storage.googleapis.com/atpls-sets/continentes/{{filename}}.jpg"
      ]
    }
  },
  "audio": {
    "bpm": 120,
    "defaults": {
      "loop": true
    },
    "signature": [
      4,
      4
    ],
    "trackMaxVoices": 1,
    "quantize": true
  },
  "control": {
    "groups": {
      "name": "audio.track"
    }
  },
  "visuals": {
    "geoMapUrl": "https://unpkg.com/world-atlas@1.1.4/world/110m.json",
    "imprint": false,
    "focus": {
      "lambda": -10,
      "verticalShift": 15,
      "scaleFactor": 1
    }
  },
  "keyboard": {
    "defaults": {
      "type": "gate"
    },
    "keyMap": {
      "q": {
        "sample": "ossie",
        "type": "toggle"
      },
      "a": {
        "sample": "orillas"
      },
      "z": {
        "sample": "mocita"
      },
      "w": {
        "sample": "nubian",
        "type": "toggle"
      },
      "s": {
        "sample": "tabla"
      },
      "x": {
        "sample": "pae"
      },
      "e": {
        "sample": "mississipi",
        "type": "toggle"
      },
      "d": {
        "sample": "camino"
      },
      "c": {
        "sample": "mariola"
      },
      "r": {
        "sample": "madinda",
        "type": "toggle"
      },
      "f": {
        "sample": "ballade"
      },
      "v": {
        "sample": "funeraire"
      },
      "t": {
        "sample": "ed-lewis",
        "type": "toggle"
      },
      "g": {
        "sample": "rueda"
      },
      "b": {
        "sample": "agnus-dei"
      },
      "y": {
        "sample": "indvumo",
        "type": "toggle"
      },
      "h": {
        "sample": "ogbeler"
      },
      "n": {
        "sample": "are-are"
      }
    }
  }
}